{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/next",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {},
    "@acmutd/portal#build": {
      "dependsOn": ["@acmutd/portal#prisma:generate"]
    },
    "@acmutd/portal#dev": {
      "dependsOn": ["@acmutd/portal#prisma:generate"]
    },
    "prisma:generate": {
      "cache": false,
      "outputs": [
        "../../node_modules/@prisma/client/**",
        "../../node_modules/.prisma/client/**",
        "node_modules/.prisma/client/**",
        "node_modules/@generated/type-graphql/**"
      ]
    },
    "prisma:push": {
      "cache": false,
      "outputs": []
    },
    "sls:invoke:local": {
      "dependsOn": ["^build"]
    },
    "sls:package": {
      "dependsOn": ["^build", "prisma:generate"],
      "outputs": [".serverless/**"]
    },
    "sls:offline": {
      "dependsOn": ["^build", "prisma:generate"]
    },
    "sls:deploy": {
      "dependsOn": ["^build", "prisma:generate"]
    },
    "sls:remove": {}
  }
}
