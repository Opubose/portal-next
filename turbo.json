{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/next",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "@acmutd/portal#build": {
      "dependsOn": ["@acmutd/portal#prisma:generate"]
    },
    "@acmutd/portal#dev": {
      "dependsOn": ["@acmutd/portal#prisma:generate"]
    },
    "prisma:generate": {},
    "prisma:push": {},
    "sls:invoke:local": {
      "dependsOn": ["^build"]
    },
    "sls:package": {
      "dependsOn": ["^build"]
    }
  }
}
