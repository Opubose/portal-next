{
  "name": "@acmutd/graphql-lambda",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:push": "doppler run -- prisma db push",
    "sls:invoke:local": "doppler run -- serverless invoke local --function email_service",
    "sls:package": "doppler run -- serverless package",
    "sls:offline": "doppler run -- serverless offline",
    "sls:offline:hot-start": "doppler run -- serverless offline --allowCache",
    "sls:deploy": "doppler run -- serverless deploy",
    "sls:remove": "doppler run -- serverless remove",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --ci --verbose",
    "format": "prettier --write '**/*.{js,ts,tsx}' && eslint --fix",
    "lint": "TIMING=1 eslint . --ext .js,.jsx,.ts,.tsx",
    "buildtest": "tsc --noEmit"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.19.0",
    "@google-cloud/storage": "^5.18.3",
    "@prisma/client": "^3.11.1",
