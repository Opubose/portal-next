    "content-disposition": "^0.5.4",
    "eslint-config-prettier": "^8.5.0",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.17.0",
    "mime-types": "^2.1.35",
    "reflect-metadata": "^0.1.13",
    "serverless": "^3.10.2",
    "source-map-support": "^0.5.19",
    "tsyringe": "^4.6.0",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
