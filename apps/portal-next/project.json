{
  "root": "apps/portal-next",
  "sourceRoot": "apps/portal-next",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/portal-next",
        "outputPath": "apps/portal-next/build"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "portal-next:build",
        "dev": true,
        "port": 4200,
        "hostname": "localhost"
      },
      "configurations": {
        "production": {
          "buildTarget": "portal-next:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "portal-next:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/portal-next"],
      "options": {
        "jestConfig": "apps/portal-next/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/portal-next/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
